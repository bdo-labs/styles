{
	"name": "styles",
	"version": "0.0.0",
	"description": "The common styles for bdo suite",
	"main": "lib/index.js",
	"style": "lib/bundle.css",
	"repository": "git://github.com/bdo-labs/styles.git",
	"license": "MIT",
	"scripts": {
		"test": "karma start"
	},
	"atomify": {
		"js": {
			"entry": "lib/index.js",
			"output": "build/build.js"
		},
		"css": {
			"entry": "lib/bundle.css",
			"output": "build/bundle.css",
			"assets": {
				"dest": "build/assets/",
				"prefix": "assets/"
			}
		}

	},
	"browserify": {
		"transform": [
			"partialify"
		]
	},
	"devDependencies": {
		"angular": "^1.2.18",
		"atomify": "^3.1.2",
		"partialify": "^3.1.1",
		"browserify": "^4.2.0",
		"karma": "^0.12.16",
		"karma-browserifast": "^0.6.1",
		"karma-jasmine": "^0.1.5"
	}
}
